{
  "clientBase": "https://icjia.illinois.gov",
  "publicPath": "/status",
  "namespace": "icjia-",
  "siteTitle": "Site Status",
  "siteTitleMobile": "Site Status",
  "deploymentURL": "https://icjia-status.netlify.com",

  "coronaInfoURL": "https://coronavirus.icjia-api.cloud/v2",
  "homeHeading": "Site STATUS",
  "statusServerURL": "https://status.icjia-api.cloud",
  "githubURL": "https://github.com/ICJIA/icjia-status",
  "siteDescription": "ICJIA real-time web status updates.",
  "searchConfig": {
    "shouldSort": true,
    "includeMatches": true,
    "tokenize": true,
    "threshold": 0.6,
    "location": 0,
    "distance": 100,
    "maxPatternLength": 32,
    "minMatchCharLength": 2,
    "keys": ["title", "summary", "headings.heading", "type", "name"]
  },
  "showCoronaInfo": true,
  "debug": true,
  "markdownItOptions": {
    "html": true,
    "xhtmlOut": false,
    "breaks": false,
    "langPrefix": "language-",
    "linkify": true,
    "typographer": false,
    "quotes": "“”‘’"
  },
  "greenStatus": [200, 204, 301, 302]
}
